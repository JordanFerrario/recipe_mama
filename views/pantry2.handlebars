<div class="stacked-box" id="box2">
    <h2>Here's Your Recipe!</h2>
    <div id="recipe-div">
        {{#each recipes}}
        <div>
            <h2>{{recipeTitle}}</h2>
            <p>{{ingredients}}</p>
            <p>{{instructions}}</p>
            <p>Posted by: {{postedBy}}</p>

        </div>
        {{/each}}
    </div>
    <button class="nes-btn" id="save-button">Save My Recipe</button>
</div>

